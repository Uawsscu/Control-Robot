#JSGF V1.0;

/**
 * JSGF Grammar for Turtle example
 */

grammar sentence;

public <move> = <train> | <command> | <question>;

<train> = this is a <obj> end ;
<command> = [jerry] <verb> <obj> | [jerry] <etc> ;
<question> = [do you know ] <obj> ;

<obj> =  ball | box | car | can | bottle | book ;
<verb> = grab | touch | pick ;
<etc> = [move the <obj> to the] [left | right] ;
